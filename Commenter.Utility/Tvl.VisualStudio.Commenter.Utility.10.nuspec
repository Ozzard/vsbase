<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>Tvl.VisualStudio.Commenter.Utility.10</id>
    <version>0.0.0</version>
    <authors>Sam Harwell</authors>
    <licenseUrl>https://raw.github.com/tunnelvisionlabs/vsbase/master/LICENSE</licenseUrl>
    <requireLicenseAcceptance>true</requireLicenseAcceptance>
    <description>
      The Commenter Utility Library provides helper classes for Visual Studio Extensions written in managed code that implement support for commenting and uncommenting code in custom languages.

      This assembly should be included in the VSIX package for your extension (this is the default behavior when you reference the package).
    </description>
    <summary>Commenter utility library for Visual Studio 2010+</summary>
    <tags>vs2010 vs2012 vs2013 vs2015</tags>
    <dependencies>
      <!-- Visual Studio 2010 -->
      <dependency id="VSSDK.IDE.10" version="[10.0.4,11.0.0)"/>
      <!--
      Exposed Dependencies (Tvl.VisualStudio.Commenter.Utility.10.dll):
        Microsoft.VisualStudio.Text.Data
        Microsoft.VisualStudio.Text.Logic
        Microsoft.VisualStudio.Text.UI
        Tvl.VisualStudio.Commenter.Interfaces
      -->
      <dependency id="VSSDK.Text" version="10.0.4"/>
      <dependency id="Tvl.VisualStudio.Commenter.Interfaces" version="1.1.0"/>
    </dependencies>
    <references>
      <reference file="Tvl.VisualStudio.Commenter.Utility.10.dll"/>
    </references>
  </metadata>
  <files>
    <!-- Assemblies -->
    <file src="bin\$Configuration$\Tvl.VisualStudio.Commenter.Utility.10.dll" target="lib\net40"/>
    <file src="bin\$Configuration$\Tvl.VisualStudio.Commenter.Utility.10.pdb" target="lib\net40"/>
    <file src="bin\$Configuration$\Tvl.VisualStudio.Commenter.Utility.10.xml" target="lib\net40"/>

    <!-- Source Code -->
    <file src="**\*.cs" exclude="obj\**\*.cs" target="src"/>
  </files>
</package>
