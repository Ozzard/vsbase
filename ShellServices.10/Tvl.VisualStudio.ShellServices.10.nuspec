<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>Tvl.VisualStudio.ShellServices.10</id>
    <version>0.0.0</version>
    <authors>Sam Harwell</authors>
    <licenseUrl>https://raw.github.com/tunnelvisionlabs/vsbase/master/LICENSE</licenseUrl>
    <requireLicenseAcceptance>true</requireLicenseAcceptance>
    <description>
      This package provides extension methods for global services available through Visual Studio's service provider interfaces.

      This assembly should be included in the VSIX package for your extension (this is the default behavior when you reference the package).
    </description>
    <summary>Visual Studio service provider extension methods for extension developers</summary>
    <tags>vs2010 vs2012 vs2013</tags>
    <dependencies>
      <!-- Visual Studio 2010 -->
      <dependency id="VSSDK.IDE.10" version="[10.0.0,11.0.0)"/>
      <!--
      Exposed Dependencies (Tvl.VisualStudio.ShellServices.10.dll):
        Microsoft.VisualStudio.ComponentModelHost
        Microsoft.VisualStudio.Language.Intellisense
        Microsoft.VisualStudio.OLE.Interop
        Microsoft.VisualStudio.Shell.Immutable.10.0
        Microsoft.VisualStudio.Shell.Interop
        Microsoft.VisualStudio.Shell.Interop.8.0
        Microsoft.VisualStudio.Shell.Interop.9.0
        Microsoft.VisualStudio.TextManager.Interop
        Microsoft.VisualStudio.TextManager.Interop.8.0
      -->
      <dependency id="VSSDK.ComponentModelHost.10" version="[10.0.0,11.0.0)"/>
      <dependency id="VSSDK.OLE.Interop.10" version="[10.0.0,11.0.0)"/>
      <dependency id="VSSDK.TextManager.Interop.10" version="[10.0.0,11.0.0)"/>
      <dependency id="VSSDK.Shell.Immutable.10" version="[10.0.0,11.0.0)"/>
      <dependency id="VSSDK.Shell.Interop.10" version="[10.0.0,11.0.0)"/>
      <dependency id="VSSDK.Language.10" version="[10.0.0,11.0.0)"/>
    </dependencies>
    <references>
      <reference file="Tvl.VisualStudio.ShellServices.10.dll"/>
    </references>
  </metadata>
