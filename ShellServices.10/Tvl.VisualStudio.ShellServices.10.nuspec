      <dependency id="VSSDK.TextManager.Interop.8" version="[8.0.4,9.0.0)"/>
    </dependencies>
    <references>
      <reference file="Tvl.VisualStudio.ShellServices.10.dll"/>
    </references>
  </metadata>
  <files>
