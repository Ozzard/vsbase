<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>Tvl.VisualStudio.Shell.Utility.11</id>
    <version>0.0.0</version>
    <authors>Sam Harwell</authors>
    <licenseUrl>https://raw.github.com/tunnelvisionlabs/vsbase/master/LICENSE</licenseUrl>
    <requireLicenseAcceptance>true</requireLicenseAcceptance>
    <description>
      The Shell Utility Library provides helper classes for Visual Studio 2012+ Extensions written in managed code that interact with the shell.

      This assembly should be included in the VSIX package for your extension (this is the default behavior when you reference the package).
    </description>
    <summary>Shell utility library for Visual Studio 2012+</summary>
    <tags>vs2012 vs2013</tags>
    <dependencies>
      <!-- Visual Studio 2012 -->
      <dependency id="VSSDK.IDE.11" version="[11.0.2,12.0.0)"/>
      <!--
      Exposed Dependencies (Tvl.VisualStudio.Shell.Utility.dll):
        Microsoft.VisualStudio.OLE.Interop
      -->
      <dependency id="VSSDK.OLE.Interop" version="[7.0.2,8.0.0)"/>
    </dependencies>
    <references>
      <reference file="Tvl.VisualStudio.Shell.Utility.dll"/>
    </references>
  </metadata>
  <files>
    <!-- Assemblies -->
    <file src="bin\vs2012\$Configuration$\Tvl.VisualStudio.Shell.Utility.dll" target="lib\net45"/>
    <file src="bin\vs2012\$Configuration$\Tvl.VisualStudio.Shell.Utility.pdb" target="lib\net45"/>
    <file src="bin\vs2012\$Configuration$\Tvl.VisualStudio.Shell.Utility.xml" target="lib\net45"/>

    <!-- Code Contracts assemblies -->
    <file src="bin\vs2012\$Configuration$\CodeContracts\Tvl.VisualStudio.Shell.Utility.Contracts.dll" target="lib\net45"/>
    <file src="bin\vs2012\$Configuration$\CodeContracts\Tvl.VisualStudio.Shell.Utility.Contracts.pdb" target="lib\net45"/>

    <!-- Source Code -->
    <file src="**\*.cs" exclude="obj\**\*.cs" target="src"/>
  </files>
</package>
